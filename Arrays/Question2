/*
* Find the sum of all elements in an array.
* Input: [1, 2, 3, 4, 5]
* Output: 15
*/
#include <stdio.h>

int main() {
 
    int a[] = {1,2,3,4,5};
    
    int len = sizeof(a)/sizeof(a[0]);
    
    int sum = 0;
    
    for(int i=0;i<len;i++)
    {
        sum = sum + a[i];
    }
    
    printf("sum: %d", sum);

}
