/*
* Find the maximum element in an array.
* Input: [3, 1, 4, 1, 5, 9]
* Output: 9
*/

#include <stdio.h>

int main() {
    
    int a[] = {3,1,4,10,5,9};
    
    int max = 0;
    
    int len = sizeof(a)/sizeof(a[0]);
    
    for(int i = 0; i < len; i++)
    {
        if(max < a[i])
        {
            max = a[i];
        }
    }
    
    printf("%d",max);
}
